            "type": "string",
            "example": "othSer",
            "description": "group identifier"
          },
          "val": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "sid": {
                  "type": "string",
                  "example": "DMT",
                  "description": "Service identifier"
